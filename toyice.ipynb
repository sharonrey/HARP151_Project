    "            return []\n",
    "        else:\n",
    "            for i in artists_info[\"artists\"][\"items\"]:\n",
    "                try:\n",
    "                    if genre in i[\"genres\"] and i[\"name\"] == artist:\n",
    "                        return i[\"id\"]\n",
