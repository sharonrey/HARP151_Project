    "    else:\n",
    "        genre_adj = genre.replace(\" \", \"%20\")\n",
    "        \n",
    "    decade = int(decade.strip(\"s\"))\n",
    "\n",
    "    if decade == 2020:\n",
    "        start_year = 2020\n",
