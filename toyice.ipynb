    "\n",
    "def get_lyrics(artist):\n",
    "    response = requests.get(\"https://api.genius.com/search\", headers=HEADERS, params={\"q\": artist})\n",
    "    data = response.json()\n",
    "\n",
    "    if not data[\"response\"][\"hits\"]:\n",
    "        print(\"No songs found.\\n\")\n",
    "        return []\n",
    "    else:\n",
    "        song = random.choice(data[\"response\"][\"hits\"])[\"result\"]\n",
