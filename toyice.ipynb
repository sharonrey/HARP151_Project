    "                    return []\n",
    "    else:\n",
    "        print(\"Enter a valid genre or try a different artist!\")\n",
    "        return []\n",
    "    \n",
    "def get_albums(genre, decade):\n",
    "    lst = []\n",
    "    albums_lst = []\n",
    "    \n",
    "    genre = genre.lower()\n",
    "    \n",
    "    if genre == \"r&b\":\n",
